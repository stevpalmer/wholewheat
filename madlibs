function wordBlanks(noun1, adjective1, place1, verbPastTense1, verbIng) {
  var result = "Once there was a " + noun1 + "named Willie.
  She was a very " + adjective1 + noun1 + ".
  She woke up one morning and wanted to visit the " + place1 + "in town,
  so she " + verbPastTense1 "her way there, " + verbIng + "all the way!";

  return result;
}

wordBlanks("dog", "silly", "library", "sniffed", "tooting");
